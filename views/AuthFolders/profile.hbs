{{>home}}

        <h1 class="teal-text">Official EduBird Profile</h1>
        <p>{{user.email}}</p>

        <h2>Your Schools: </h2>


        {{#each databaseData}}
            
             <ul class="collection">

    <li class="collection-item avatar">
      <i class="material-icons circle teal"></i>
      <span class="title">{{{name}}}</span>
      <p>{{{description}}} <br>
    
      </p>
      <a href="/school/main/info/{{{secretSchoolCode}}}" class="secondary-content"><i class="material-icons">infos</i></a>
    </li>
    </ul>
    
                 
        {{/each}}


         
<script src="https://ajax.googleapis.com/ajax/libs/jquery/3.3.1/jquery.min.js"></script>
<script src="https://cdnjs.cloudflare.com/ajax/libs/materialize/1.0.0/js/materialize.min.js"></script>

  <!-- Element Showed -->
  <div class="fixed-action-btn">
  <a class="pulse btn-floating btn-large blue">
    <i class=" large material-icons">menu</i>
  </a>
  <ul>
    <li><a class="btn-floating red"><i class="material-icons">mode_edit</i></a></li>
    <li><a class="btn-floating btn-medium yellow darken-1"><i class="material-icons">format_quote</i></a></li>
    <li><a class="btn-floating green"><i class="material-icons">publish</i></a></li>
    <li><a class="btn-floating blue"><i class="material-icons">attach_file</i></a></li>
  </ul>
</div>
<script>

  $(document).ready(function(){
    $('.fixed-action-btn').floatingActionButton();
  });
</script>
<style>

    body {
        text-align: center;
    }
</style>
    </body>
</html>